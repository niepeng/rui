#leftMenus {
    background:#f9f9f9;
    float: left;
    border: 1px solid #ddd;
    border-top: 0 none;
}
#leftMenus .side-list{
    margin:0;
}
#leftMenus .side-list dt{
    border-top:1px solid #ddd;
}
#leftMenus .side-list dt a{
    display: block;
    height:30px;
    line-height: 30px;
    text-align: left;
    font-size: 12px;
    padding-left:22px;
    margin-left:12px;
    background: url(../images/icons.png) -5px 8px no-repeat;
}
