.flowstep .step-cur .step-no {
	background-position: 50% -93px;
}
.flowstep .step-first .step-no {
	background-position:50% -16px;
}
.flowstep .step-first .step-done .step-no {
	background-position:50% -46px;
}
.flowstep .step-last .step-no {
	background-position:50% -77px;
}
.flowstep .step-last .step-cur .step-no{
	background-position:50% -31px;
}
.flowstep .step-done .step-name,.flowstep .step-cur .step-name,.flowstep .step-first .step-name{
	color:#e61e25;
}
