 *{margin:0;padding:0}
 html,body{background:url(../images/bodybg.jpg) repeat}
 ol, ul { list-style: none outside none;}
 :focus { outline: 0 none;}
 a img {  border: 0 none;}
 img { vertical-align: middle;border:0 none}
 .uwrap{}
 .u{width:810px;margin:0 auto;position:relative;margin-top:120px;background:url(../images/login-uxiang.jpg) no-repeat;height:300px;padding-bottom:40px;}
 .login{position:absolute;right:0;top:-10px;width:390px;height:404px;background:url(../images/login-form.png) no-repeat}
 .login .fore1{height:115px}
 .login .fore2,.login .fore4{height:40px;padding:0 0 0 50px}
 .login .fore2 input,.login .fore4 input {padding:3px 3px 3px 35px;width:250px;height:34px;background:transparent;border:0 none}
 .login .fore3,.login .fore5{height:20px;padding-left:60px;color:#c00;font-size:12px}
 .login .fore6{padding:0 0 0 40px}
 .login .fore6 .btn-logining {width:310px;height:90px;background:url(../images/login-form-btn01.jpg) no-repeat;display:block;border:0 none;padding:20px;cursor:pointer}
 .login .fore6 .btn-logining:hover{background:url(../images/login-form-btn01hover.jpg) no-repeat}
.color-orange{color:#f60;}