  .order_detail_title{
   padding: 20px 5%;
   border-bottom: solid 10px #ce0d0d;
  }
  .order_detail_title p{
   padding: 20px 0;
   font-size: 20px;
  }
  .order_detail_title p:first-child{
   font-size: 24px;
   font-weight: bold;
  }
  .order_detail_con{padding: 0 5%;}
  .order_detail_con .order_detail{
   margin: 15px 0;
  }
  .order_detail_con h3{
   font-size:24px;
   color:#676579;
   padding-top: 20px;
  }
  .order_detail_con p{padding:10px;}
  .order_detail_con .order_detail li{
   padding: 10px 20px;
   border-bottom: solid 1px #cdcdcd;
  }
  .od_img{
   width: 20%;
   height: 100px;
   overflow: hidden;
  }
  .od_list {
   width: 80%;
  }
  .od_borTop{
   border-top: solid 1px #cdcdcd;
  }
  .od_address p{padding: 4px 10px;}
  .od_btn_gray{
   display: block;
   padding: 10px 20px;
   margin-right: 5%;
   float: right;
   color: #000;
   border:solid 1px #cdcdcd;
   border-radius: 3px;
   -moz-box-shadow:0px 3px 10px rgba(0,0,0,0.3);               
   -webkit-box-shadow:0px 3px 10px rgba(0,0,0,0.3);           
   box-shadow:0px 3px 10px rgba(0,0,0,0.3); 
   margin-bottom: 20px;
  }