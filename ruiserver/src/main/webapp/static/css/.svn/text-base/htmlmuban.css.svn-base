}
#page TD {
    color: #666666;
    font-family: Verdana,Arial,Helvetica,sans-serif;
    font-size: 11px;
}
.webtxt {
    clear: both;
    font-size: 14px;
    line-height: 160%;
    margin: 12px;
    overflow: hidden;
    text-align: justify;
    word-break: break-all;
    word-wrap: break-word;
}
.sbt {
    border: 1px solid #7F9DB9;
    font-family: Verdana,Arial,Helvetica,sans-serif;
    font-size: 12px;
