    font-size: 12px;
    height: 22px;
}
A.btlink:link, A.btlink:visited {
    color: #006600;
    text-decoration: none;
}
A.btlink:hover {
    color: #CC0000;
    text-decoration: underline;
}
ul.TabBarLevel {
    background-image: url("../images/tabbar_level1_bk.gif");
    height: 29px;
    list-style: none outside none;
    margin: 0;
    padding: 0;
}
ul.TabBarLevel li {
    background: url("../images/tb_left_bk.gif") no-repeat scroll left top transparent;
    float: left;
    height: 29px;
    margin-right: 1px;
    padding: 0;
}
ul.TabBarLevel li a {
    background: url("../images/tb_right_bk.gif") no-repeat scroll right top transparent;
    color: #000066;
    display: block;
    line-height: 29px;
    padding: 0 20px;
    white-space: nowrap;
}
ul.TabBarLevel li.Selected {
    background: url("../images/tb_selected_left_bk.gif") no-repeat scroll left top transparent;
}
ul.TabBarLevel li.Selected a {
    background: url("../images/tb_selected_right_bk.gif") no-repeat scroll right top transparent;
}
ul.TabBarLevel li a:link, ul.TabBarLevel li a:visited {
    color: #000066;
    text-decoration: none;
}
ul.TabBarLevel li a:hover, ul.TabBarLevel li a:active {
    color: #0033CC;
    text-decoration: none;
}
ul.TabBarLevel li.Selected a:link, ul.TabBarLevel li.Selected a:visited {
    color: #CC0000;
    font-weight: bold;
    text-decoration: none;
}
ul.TabBarLevel li.Selected a:hover, ul.TabBarLevel li.Selected a:active {
    color: #CC0000;
    text-decoration: none;
}
