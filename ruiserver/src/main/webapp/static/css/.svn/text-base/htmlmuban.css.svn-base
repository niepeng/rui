
body {
    margin: 0;
}
html {
    border: 0 none;
}
html {
    overflow-x: hidden;
    overflow-y: auto;
}
TD {
    font-size: 12px;
}
a:link, a:visited {
    color: #000066;
    text-decoration: none;
}
a:hover {
    color: #CC0000;
    text-decoration: underline;
}
.headbody {
    background: url("../images/HeadBack.jpg") repeat scroll 0 0 transparent;
}
.headmain {
    border-bottom: 1px solid #014B56;
