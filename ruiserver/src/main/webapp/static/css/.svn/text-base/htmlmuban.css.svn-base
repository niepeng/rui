}
.menuA:link, .menuA:visited {
    background: url("../images/menubutton.jpg") no-repeat scroll 4px 0 transparent;
    color: #FFFFFF;
    display: block;
    font-size: 12px;
    height: 35px;
    text-decoration: none;
    width: 100%;
}
.menuA:hover {
    background: url("../images/menubutton.jpg") no-repeat scroll 4px -36px transparent;
    color: #FFFFFF;
    text-decoration: none;
}
.menuAS:link, .menuAS:visited {
    background: url("../images/menubutton.jpg") no-repeat scroll 4px -72px transparent;
    color: #000000;
    display: block;
    font-size: 12px;
    font-weight: bold;
    height: 35px;
    text-decoration: none;
    width: 100%;
}
.ContentBody {
    background: url("../images/mainbg.gif") repeat scroll 0 0 #F6F6F6;
    margin: 15px;
}
