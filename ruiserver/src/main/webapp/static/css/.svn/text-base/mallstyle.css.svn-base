#leftMenus dd {
/*    background-color: #F4F9FF;
*/    background-image: url("../images/kit.gif");
    background-position: 22px 10px;
    background-repeat: no-repeat;
    line-height: 25px;
    padding-left: 34px;
    margin:0;
}
#leftMenus dd a {
    color: #3D6BAC;
    text-decoration: none;
}
.ly-main{
    padding:0px 10px 0 110px;
}
#right {
    overflow-y: auto;
}
#right ifram {
    height: 100%;
    width: 100%;
}
.dmenu {
    float: right;
}
.dmenu li {
    float: left;
}
#right_content {
    overflow-y: auto;
}
#short_msg {
    border: 1px solid #C4D5E0;
    color: #000000;
    float: left;
    height: 30px;
    left: 550px;
    line-height: 30px;
    position: absolute;
    text-align: center;
    top: 20px;
    width: 250px;
}
#short_msg .msglist {
    background-color: #F2F9FD;
    border: 1px solid #DEEEFA;
    display: none;
    left: 0;
    position: relative;
    width: 250px;
    z-index: 999999;
}
#short_msg .msglist li {
    border-bottom: 1px dashed #D8ECFF;
    height: 25px;
    line-height: 25px;
    width: 250px;
}
#short_msg .msglist li .name {
    width: 150px;
}
#short_msg .msglist li .link {
    width: 150px;
}
#short_msg .newmessage {
    color: red;
}
#msg_close_btn {
    display: none;
}
#msg_view_btn {
    display: none;
}
.pg-body{
    overflow: hidden;
    zoom:1;
}
.pg-header{
    width: 100%;
    height: 30px;
    line-height: 30px;
    border:1px solid #ddd;
    margin:20px auto 0;
    overflow: hidden;
    background: #f9f9f9;
    zoom:1;
}
.pg-header .title{
    float:left;
    padding:0 2em;
    font-size: 14px;
    background: #eee;
    border-right: 1px solid #ddd;
}
.pg-header .subtitle{
    float:left;
    padding:0 1em;
    font-size: 12px;
    background: #f9f9f9;
    border-right: 1px solid #ddd;
}
.pg-header .content{
    padding-right:6px;
    float: right;
}

.pg-content{
    margin-top:10px;
}
.tree-actions{
    margin-bottom:10px;
}

/*ui*/
.red{
    color:red;
}
.ui-gray{
    color:#999;
}

.ui-yellow-btn{
    padding:0 1em;
    font: 12px/30px '';
    border: 1px solid #efb029;
    color:#d63239;
    filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr='#fee491',endcolorstr='#fbc966',gradientType=0);
    -ms-filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr='#fee491',endcolorstr='#fbc966',gradientType=0);
    background: -webkit-gradient(linear, 0 0, 0 bottom, from(#fee491), to(#fbc966));
    background: -moz-linear-gradient(top, #fee491, #fbc966);
    background: -o-linear-gradient(top,  #fee491, #fbc966);
    border-radius: 2px;
    margin:0 6px;
}
.ui-gray-btn{
    padding:0 1em;
    font: 12px/30px '';
    border: 1px solid #ccc;
    color:#444;
    filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr='#ffffff',endcolorstr='#f2f2f2',gradientType=0);
    -ms-filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr='#ffffff',endcolorstr='#f2f2f2',gradientType=0);
    background: -webkit-gradient(linear, 0 0, 0 bottom, from(#ffffff), to(#f2f2f2));
    background: -moz-linear-gradient(top, #ffffff, #f2f2f2);
    background: -o-linear-gradient(top, #ffffff, #f2f2f2);
    border-radius: 2px;
    margin:0 6px;
}
/*page*/
.pg-edit-text{
    width: 200px;
    height: 20px;
    padding: 2px;
    font-size: 14px;
    line-height: 20px;
    margin-right:6px;
}
.pg-table{
    margin-top:20px;
    font: 14px/20px '';
}
.pg-table td,.pg-table th{
    padding:5px;
    vertical-align: middle;
}
.pg-edit-table{
    margin-top:20px;
    font: 14px/24px '';
}
.pg-edit-table td{
    padding:5px;
    
}
.pg-list-table{
    width: 100%;
    margin: 20px auto;
}
.pg-list-table td, .pg-list-table th,.pg-cate-table th{
    padding:10px;
    border:1px solid #ddd;
    text-align: center;
    vertical-align: middle;
}
.pg-list-table th,.pg-cate-table th{
    background: url("../images/bg1px.png") repeat-x scroll 0 -151px transparent;
    font:12px/30px '';
    padding: 0 12px;
}
.pg-list-table .image{
}
.pg-list-table .image img{
    vertical-align: middle;
}
.pg-cate-table{
    width: 600px;
}
.pg-cate-table td{
    padding:10px 0;
}
.pg-cate-table select{
    width: 199px;
    height:30px;
    font: 14px/30px '';
}