      
html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, code, del, dfn, em, img, q, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td {
    border: 0 none;
    font-family: inherit;
    font-size: 100%;
    font-style: inherit;
    margin: 0;
    padding: 0;
    vertical-align: baseline;
}
body {
    color: #222222;
    font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
    font-size: 12px;
    line-height: 1.5;
}
a {
    color: #000000;
    text-decoration: none;
}
ul {
    list-style-type: none;
}
#head {
    background: url("../images/bg1px.png") repeat-x scroll 0 0;
    color: #FFFFFF;
    height: 151px;
    margin: 0 auto;
}
#head .head-top{
    background: url("../images/ws-header.gif") center;
    height: 75px;
    padding: 20px;
}
#head .head-top .logo{
    float:left;
}
#head .head-top .pg-user{
    float:right;
    margin-top: 50px;
    color:#fff;
    font-size: 16px;
}
.pg-user a{
    color:#eee;
}
#head .head-top h1{
    font:24px/50px '';
    color:#fcefd7;
}
#head .top {
    height: 151px;
    margin: 0 auto;
}
#head .top .sysmenu {
    float: right;
    padding-right: 30px;
}
#head .top .sysmenu {
    height: 29px;
    line-height: 29px;
}
#head .sysmenu ul {
    margin-top: 20px;
}
#head .sysmenu li {
    background: url("../images/buttons.gif") no-repeat scroll -16px -80px transparent;
    display: block;
    float: left;
    height: 32px;
    margin-left: 5px;
    text-align: center;
    width: 62px;
}
#head .appmenu {
    float: left;
    height: 35px;
    margin-top: 1px;
    padding: 0 20px;
    padding-left:100px;
}
#head .appmenu ul {
    border-left:1px dotted #333;
    overflow: hidden;
    zoom:1;
}
#head .appmenu li {
/*    background: url("../images/buttons.gif") no-repeat scroll -10px -3px transparent;
*/    display: block;
    height: 35px;
    
    float: left;
    border-right:1px dotted #333;
    line-height: 35px;
    text-align:center;
}
#head .appmenu li a {
    display: block;
    padding:0 20px;
    font: 600 14px/35px "Simsun";
    color: #fff;
}
#head .appmenu li span {
    color: #ffffff;
    font-size: 14px;
    line-height: 34px;
}
a {
    outline: 0 none;
}
#head .appmenu .current {
/*    background: url("../images/buttons.gif") no-repeat scroll -3px -38px transparent;
*/  background:#fff;
    border-right: 0 none;
}
#head .appmenu .current a {
    color:#000;
}
#head .appmenu li.current span {
    color: #000000;
    font-size: 14px;
}
#head .logo {
    float: left;
    height: 55px;
    margin-left: 10px;
    margin-top: 10px;
    overflow: hidden;
    position: absolute;
}
#leftMenus {
    width: 100px;
    background:#f9f9f9;
    min-height: 500px;
    float: left;
    border-right: 1px solid #ddd;
}
#leftMenus .side-list{
    min-height: 500px;
    margin-top: 20px;
}
#leftMenus .side-list dt{
    border-top:1px solid #ddd;
}
#leftMenus .side-list dt a{
    display: block;
    height:30px;
    line-height: 30px;
    text-align: left;
    font-size: 12px;
    padding-left:22px;
    margin-left:12px;
    background: url(../images/icons.png) -5px 8px no-repeat;
}
