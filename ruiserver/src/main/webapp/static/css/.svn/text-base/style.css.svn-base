}
.download span{
	padding: 0;
}
.download b{
	width: 16px;
	height: 16px;
	display: inline-block;
	margin: 6px 2px 6px 0;
	background: url(../images/global_sprite.png) no-repeat -253px -5px;
} 

/*微信管理 回复管理*/
.mask{
  display:none;
  width:100%;
  height:100%;
  position:absolute;
  top:0;
  left:0;
  background-color: #000;
  opacity: 0.5;
  filter: alpha(opacity=50);
}
.poptuwen{
  display:none;
  width:1000px;
  height:500px;
  position:absolute;
  top: 50%;
  left: 50%;
  margin-top: -250px;
  margin-left: -500px;
}
.poptuwen span{
  display: block;
  width: 10px;
  height: 10px;
  padding: 5px;
  border-radius: 10px;
  background-color: #ccc;
  position: absolute;
  top: 0;
  right: 0;
  text-align: center;
  line-height: 10px;
  cursor: pointer;
} 

