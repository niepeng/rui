/*删除按钮点击弹框*/
.dialog-inner,.cenmsg{
	border:1px solid #c4c4c4;
	width: 200px;
	position: absolute;
	background-color: #fff;
	-webkit-box-shadow:0 0 5px rgba(122,122,122,0.8);
	-moz-box-shadow:0 0 5px rgba(122,122,122,0.8);
	-ms-box-shadow:0 0 5px rgba(122,122,122,0.8);
	-o-box-shadow:0 0 5px rgba(122,122,122,0.8);
	top:100px;
	left: 100px;
}
.cenmsg{
	position: fixed;
	top:50%;
	left: 45%;
}
.dialog-inner .dialog-toolbar,.cenmsg .dialog-toolbar{
	height: 25px;
	border-top:1px solid #fff;
	border-bottom: 1px solid #c4c4c4;
	padding: 0 10px;
	background-color: #f3f3f3;
}
.clearfix{display: block;}
.dialog-inner .dialog-toolbar .dialog-close,.cenmsg .dialog-toolbar .dialog-close{
	width: 13px;
	height: 13px;
	margin-top: 6px;
	float: right;
	text-indent: -9999px;
	line-height: 0;
	overflow: hidden;
	background: url('../img/dialog-close.gif') no-repeat;
}
.dialog-inner .dialog-toolbar .dialog-title,.cenmsg .dialog-toolbar .dialog-title{
	float: left;
	line-height: 25px;
}
.dialog-inner .dialog-content,.cenmsg .dialog-content{padding: 10px;}
.dialog-inner .dialog-content .cart-delete,.cenmsg .dialog-content .cart-delete{
	color: #c00;
	line-height: 22px;
	text-align: center;
}
.dialog-inner .dialog-content .btns{
	overflow: auto;
	padding: 10px 0 0 40px;
}
.dialog-inner .dialog-content .btns .btn{
	float: left;
	display: block;
	overflow: hidden;
	position: relative;
	margin-right: 5px;
	padding-right: 10px;
	line-height: normal;
	font-size: 12px;
	background: url('../img/btn-bg-1.png') no-repeat 100% -63px;
	color: #666;
}
.dialog-inner .dialog-content .btns a.btn:hover{color: #333;}
.dialog-inner .dialog-content .btns .btn .btn-text{
	height: 21px;
	padding: 0 0 0 10px;
	float: left;
	display: block;
	overflow: hidden;
	line-height: 22px;
	text-align: left;
	background-position: 0 0;
	background: url('../img/btn-bg-1.png') no-repeat;
}
/*删除按钮弹框结束*/

/*会员信息*/
.btn-gray,.btn-red,.btn-green{
	cursor: pointer;
	display: inline-block;
}
.btn-gray{
	background: #ededed;
	filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr='#fcfcfc',endcolorstr='#ededed',gradientType=0);
	-ms-filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr='#fcfcfc',endcolorstr='#ededed',gradientType=0);
	background: -webkit-gradient(linear, 0 0, 0 bottom, from(#fcfcfc), to(#ededed));
	background: -moz-linear-gradient(top, #fcfcfc, #ededed);
	background: -o-linear-gradient(top,  #fcfcfc, #ededed);
}
.btn-gray:hover{
	background: #fcfcfc;
	filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr='#ededed',endcolorstr='#fcfcfc',gradientType=0);
	-ms-filter:progid:DXImageTransform.Microsoft.gradient(startcolorstr='#ededed',endcolorstr='#fcfcfc',gradientType=0);
	background: -webkit-gradient(linear, 0 0, 0 bottom, from(#ededed), to(#fcfcfc));
	background: -moz-linear-gradient(top, #ededed, #fcfcfc);
	background: -o-linear-gradient(top,  #ededed, #fcfcfc);
}
.cen-top{
	height: 86px;
	margin: 12px 0 42px;
    border:solid 2px #4367a3;
    background-color: #ecf5fa;
}
.cen-top dt{
   width: 110px;
   float: left;
   font: bold 20px/80px '';
   margin-right: 140px;
   text-align: center;
}
.cen-top dd{
	position: relative;
	float: left;
	padding: 20px 15px 0;
}
.cen-top dd span{
	font: bold 36px/30px '';
	color: #3f5db5;
}
.cen-top dd s{
	display: block;
	font: 12px/20px '';
	color: #a7a8ad;
	text-align: center;
	text-decoration: none;
}
.cen-top dd i{
	position: absolute;
	top:22px;
	right: -4px;
	width: 20px;
	height: 24px;
	background: url(../images/global_sprite.png) no-repeat -209px 0;
}
.cen-top dd i.line{
	width: 12px;
	height: 0;
	top:30px;
    border:solid 2px #9b999a;
}
.cen-top dd i.down{
	background-position: -229px 0;
}
.cen-img{
	height: 502px;
	margin-bottom: 40px;
}
.showimg{
	width: 858px;
	height: 500px;
	overflow: hidden;
	border:solid 1px #ddd;
	margin-bottom: 22px;
}
.cen-img span{
	position: relative;
	padding: 0 20px 0 30px;
}
.cen-img i,.cen-img s{
	width: 22px;
	height: 0;
	position: absolute;
	top:8px;
	left: 0;
	display: inline-block;
	border: solid 1px #1973af;
}
.cen-img s{
	border-style: dashed;
}
.download{
	display: block;
    height: 26px;
    font: 14px/26px '';
