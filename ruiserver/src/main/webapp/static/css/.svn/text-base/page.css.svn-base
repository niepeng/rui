}
.pagin a, .pagin span {
	float: left;
	height: 20px;
	line-height: 20px;
	margin-left:4px;
	padding:2px 7px;
	overflow: hidden;
	font-size: 12px;
	font-family: arial;
}
.pagin .text-jumpto {
	float: left;
	width:33px;
	height:18px;
	margin: 0 5px;
	padding: 1px;
	border-color: #aaa #ddd #ddd #aaa;
	border-style: solid;
	border-width: 1px;
	text-align: center;
	font-family: verdana;
}
.pagenation li{
	display: inline-block;
	position: relative;
	width: 40px;
	margin-left: 5px;
	margin-right: 5px;
}
.pagenation  .bg {
	border: 1px solid #c1c1c1;
	width: 38px;
	height: 38px;
	position: absolute;
	bottom: 0;
	left: 0;
	border-radius: 2px;
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=white,endColorstr=#f0f0f0);
	background-image: -moz-linear-gradient(top,#fff,#f0f0f0);
	background-image: -webkit-linear-gradient(top,#fff,#f0f0f0);
	background-image: -ms-linear-gradient(top,#fff,#f0f0f0);
	overflow: hidden;
}
.pagenation .page-w a {
	padding: 11px 15px;
	font-family: Arial,SimSun;
	font-size: 14px;
	line-height: 40px;
	position: relative;
	z-index: 1;
	color: #666;
}
.pagenation .page-next,.pagenation .page-pre{
	width:auto;
} 
.pagenation .page-w .page_num {
	padding: 0;
	display: block;
}
.pagenation .page-pre, .pagenation .page-next {
	color: #9d9d9d;
	border-radius: 2px;
	border: 1px solid #c1c1c1;
	filter: progid:DXImageTransform.Microsoft.gradient(GradientType=0,startColorstr=white,endColorstr=#f0f0f0);
	background-image: -moz-linear-gradient(top,#fff,#f0f0f0);
	background-image: -webkit-linear-gradient(top,#fff,#f0f0f0);
	background-image: -ms-linear-gradient(top,#fff,#f0f0f0);
}
.pagenation .page-curr, .pagenation .page-hover {
	position: relative;
	border: 1px solid #C80007;
	background: #FFAAAD;
	font-size: 14px;
	border-radius: 2px;
	color:#C80007;
	display: block;
	width: 38px;
	height: 38px;
	line-height: 38px;
	overflow: hidden;
}
.pagenation .hover {
	border: 1px solid #908f8f;
}
.item-list {
	margin:60px 0;
}
.item-list .pagin {
