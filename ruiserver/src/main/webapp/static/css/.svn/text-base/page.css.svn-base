/***搜索页***/
.ui-icon {
	display: inline-block;
	overflow: hidden;
	vertical-align: middle;
}
.side-nav {
	width:200px;
}
.side-nav .ui-category {
	border:solid 1px #dcdcdc;
	border-bottom:none;
	background:#f2f2f2;
}
.side-nav .ly-category h2 ,.side-nav .ly-category dt,.ly-item-rank h3{
	height:28px;
	line-height:28px;
	padding:0 7px 0 10px;
	border-bottom:solid 1px #dcdcdc;
}
.side-nav .ly-category dt {
	border-top:solid 1px #fff;
	cursor:pointer;
}
.side-nav .ui-category b,.main .crumbs-nav li .ui-icon ,.main .filter dd b,.top_pagi a, .top_pagi span,.btn-add .btn-reduce,.btn-add .btn-plus,.btn-add button,.pagin .btn-skipsearch ,.tb-detail .property .promo .ui-promo,.tb-detail .property .store-select b,.tb-detail .property .ly-choose .btn-atten a{
	background:url(../images/s-pg-ui.png) no-repeat;
}
.side-nav .ui-category b {
	float:left;
	width:15px;
	height:16px;
	margin:7px 8px 0 0;
	background-position:0 -19px;
}
.side-nav .ly-category dd {
	display:none;
}
.side-nav .ly-category dl.hover {
	border-bottom:solid 1px #dcdcdc;
}
.side-nav .ly-category dl.hover dt b {
	background-position:0 0;
} 
.side-nav .ly-category dl.hover dd {
	display:block;
	padding:3px 15px 3px 35px;
	background:#fff;
}
.side-nav .ly-item-rank {
	border:solid 1px #dcdcdc;
}
.side-nav .ly-item-rank h3{
	background:#f2f2f2;
}
.side-nav .ly-item-rank .first {
	border-top:none;
}
.side-nav .ly-item-rank li {
	padding:14px;
	border-top:dashed 1px #ddd;
}

.main{
	width:780px;
}
.main .crumbs-nav {
	height:27px;
	line-height:27px;
	padding-right:8px;
	border:solid 1px #dcdcdc;
	background:#F6F6F6;
}
.main .crumbs-nav p {
	float:right;
	color:#999;
}
.main .crumbs-nav p span {
	padding:0 2px;
	color:#EB1C24;
}
.main .crumbs-nav ul {
	float:left;
	height:27px;
}
