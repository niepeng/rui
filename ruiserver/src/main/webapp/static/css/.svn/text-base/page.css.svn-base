	right:12px;
	width:7px;
	height:10px;
	background-position:-16px -3px;
}
.main .filter .up b {
	background-position:-24px -3px;
}
.main .filter .down b {
	background-position:-31px -3px;
}
.top_pagi {
	margin-right:5px;
}
.top_pagi a, .top_pagi span{
	float: left;
	width:18px;
	height:18px;
	margin:4px 5px 0 0;
}
.top_pagi p {
	padding:0 10px;
}
.top_pagi .disable-pre{
	background-position:-45px -49px;
	cursor:default;
}
.top_pagi .disable-next{
	background-position:-102px -49px;
	cursor:default;
}
.top_pagi .pre{
	background-position:-83px -49px;
}
.top_pagi .next{
	background-position:-64px -49px;
}
.list li {
	width:178px;
	padding:7px 0 0;
	margin-left:20px;
	border:solid 1px #fff;
}
.list li .p-img ,.list li .p-price {
	padding:0 7px;
}
.list li .p-img{
