	margin-top:6px;
}
.btn-add *{
	vertical-align:middle;
}
.list .first {
	margin-left:0;
}
.btn-add .text-num {
	width: 28px;
	height: 16px;
	border: 1px solid #CFCFCF;
	color: #262626;
	text-align: center;
	vertical-align: middle;
}
.btn-add .btn-plus,.btn-add .btn-reduce {
	width:15px;
	height:15px;
}
.btn-add .btn-reduce{
	background-position:0 -43px;
}
.btn-add .btn-plus{
	margin:0 5px 0 1px;
	background-position:0 -61px;
}
.btn-add button {
	width:82px;
	height:22px;
	background-position:-39px -1px;
