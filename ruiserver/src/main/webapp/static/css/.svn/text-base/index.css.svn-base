.index-head .banner .slide{
	height: 360px;
	overflow: hidden;
	position: relative;
}
.slide >a{
	width: 46px;
	height: 78px;
	position: absolute;
	top:50%;
    margin-top: -39px;
    background: url(../img/index_sprite.png) no-repeat -42px -407px;
}
.slide >a.prev{
   left:0;
}
.slide >a.next{
   right:0;
   background-position: -86px -407px;
}
.index-head .banner .slide .pager{
	position: absolute;
	left: 50%;
	bottom: 16px;
}
.index-head .banner .slide .pager a{
	float: left;
	width: 14px;
