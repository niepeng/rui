/* CSS reset */
html, body, div, iframe, h1, h2, h3, h4, h5, h6, p, pre,strong, dl, dt, dd, ol, ul, li, fieldset, form, label, legend,th,td {
	margin: 0;
	padding: 0;
}
ol, ul {
	list-style: none outside none;
}
:focus ,a:focus,input[type=submit]{
	outline: 0 none;
}
fieldset,a img {
	border: 0 none;
}
fieldset,img {
	vertical-align: middle;
	border:0 none;
}
table {
	empty-cells: show;
	border-collapse: collapse;
	border-spacing: 0;
}
address,caption,cite,code,dfn,em,th,var {
	font-weight: normal;
	font-style: normal;
}
a:link, a:visited {
	color: #666;
	text-decoration: none;
}
a:hover {
	color: #e61e25;
	text-decoration: none;
}
input:-webkit-autofill {
	-webkit-box-shadow: 0 0 0px 1000px white inset;
}
body {
	color: #333;
	font: 12px/150% Arial,Verdana;
}
.overflow {
	overflow: hidden;
}.hide {
	display: none;
}
.worn {
	color:#E61E25;
}
h1 {
	font: 20px "microsoft yahei","";
}h2, h3 {
	font-size: 14px;
}h4, h5, h6 {
	font-size: 12px;
}
.ly-w{
	width:1000px;
	margin:0 auto;
}
.ly-mb10{
	margin-bottom:10px;
}
.ly-mb15{
	margin-bottom:15px;
}
.ly-mb30{
	margin-bottom:30px;
}
.f14{
	font-size:14px;
}.f18{
	font-size:18px;
}
.m, .mt, .mc, .mb{
	overflow: hidden;
}
.mt, .smt {
	cursor: default;
}
.mt h2, .smt h3 {
	font-family: "microsoft yahei";
}
.curr .mt, .curr .smt {
	color: #000;
}
.curr .mc, .curr .smc {
	display: block;
}
.m .mt .extra a:link, .m .mt .extra a:visited, .sm .smt .extra a:link, .sm .smt .extra a:visited {
	color: #005EA7;
}

.form label, .form input, .form select, .form textarea, .form button, .form .label {
	float: left;
	font-size: 12px;
}
.fl {
	float: left;
}
.fr {
	float: right;
}
.fn {
	float: none;
}
.al {
	text-align: left;
}
.ac {
	text-align: center;
}
.ar {
	text-align: right;
}

.color-orange {
	color:#f60;
}
.color-red{
	color:#f00;
}
.error-tip{
	color:#f00;
}

.clearfix:before,.clearfix:after{
	content:"";
	display:table;
}
.clearfix:after{
	clear:both;
}
.clearfix{
	*zoom:1;
}
.clear{
	clear:both;
	display:block;
	overflow:hidden;
	visibility:hidden;
	height:0;
} 
* + html .clearfix {
	min-height: 1%;
}

.lh {
	overflow: hidden;
	zoom:1;
}

.lh li, .lh dl, .lh .item {
	float: left;
}
.ld {
	position: relative;
}
.ld s, .ld b{
	overflow: hidden;
	position: absolute;
}
.line-through {
	text-decoration:line-through;
}
.p-img, .p-name, .p-price, .p-market, .p-detail {
	overflow: hidden;
}
.p-img {
	padding: 5px 0;
}
.p-img a:link, .p-img a:visited {
	text-decoration: none;
}
.p-img .pi {
	cursor: pointer;
}
.p-name, .p-detail {
	word-break: break-all;
	word-wrap: break-word;
}
.p-price {
	color: #999999;
}
.p-price strong, .p-market strong {
	color: #000;
}
.pi {
	background-image: url(x.jpg);
	background-repeat: no-repeat;
	color: #FFF;
	font-family: "microsoft yahei";
	font-size: 16px;
	height: 40px;
	line-height: 31px;
	overflow: hidden;
	padding: 12px 0 0;
	position: absolute;
	right: 0;
	top: 10px;
	width: 52px;
}
.pix0 {
	background-position: 0 0;
}
.pix1 {
	background-position: 0 0;
}
